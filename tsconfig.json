{
  "compileOnSave": false,
  "compilerOptions": {
    "incremental": true,
    "outDir": "h5p-integrationintegration",
    "removeComments": true,
    "module": "esnext",
    "target": "es5",
    "moduleResolution": "node",
    "esModuleInterop": false,
    "sourceMap": false,
    "alwaysStrict": true,
    "allowJs": false,
    "declaration": true,
    "allowSyntheticDefaultImports": true,
    "lib": ["DOM", "ES2015"]
  },
  "exclude": [
    "node_modules",
    "h5p-integration/*tegration/*",
    ".idea/*",
    ".git/*"
  ]
}
